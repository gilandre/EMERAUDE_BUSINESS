{
  "dashboard": {
    "title": "Emeraude Business - Database Performance",
    "tags": ["emeraude", "database"],
    "panels": [
      {
        "title": "DB Queries Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(db_queries_total[5m])) by (operation)",
            "legendFormat": "{{operation}}"
          }
        ]
      },
      {
        "title": "DB Query Duration (p95)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(db_query_duration_seconds_bucket[5m])) by (le, operation))",
            "legendFormat": "{{operation}}"
          }
        ]
      }
    ]
  }
}
