{
  "dashboard": {
    "title": "Emeraude Business - Cache Performance",
    "tags": ["emeraude", "cache"],
    "panels": [
      {
        "title": "Cache Hit Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(cache_hits_total[5m])) / (sum(rate(cache_hits_total[5m])) + sum(rate(cache_misses_total[5m])))",
            "legendFormat": "Hit rate"
          }
        ]
      },
      {
        "title": "Cache Hits vs Misses",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(cache_hits_total[5m])) by (key_prefix)",
            "legendFormat": "hits {{key_prefix}}"
          },
          {
            "expr": "sum(rate(cache_misses_total[5m])) by (key_prefix)",
            "legendFormat": "misses {{key_prefix}}"
          }
        ]
      }
    ]
  }
}
